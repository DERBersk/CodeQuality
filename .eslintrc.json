{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
    ],
    "rules": {
        "indent": [
            "error",
            "tab"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "id-length": [
            "error",
            {
                "min": 3,
                "max": 20
            }
        ],
        "no-multiple-empty-lines": "error",
        "camelcase": "error",
        "keyword-spacing": "error"
    }
}
